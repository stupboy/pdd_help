{
    "name": "pdd商品管理",
    "manifest_version": 2,
    "version": "1.0",
    "description": "pdd商品获取辅助工具",
    "browser_action": {
      "default_icon": "image/showpicture.png",
      "default_title": "pdd商品管理",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["js/background.js"]
    },
    "content_scripts": [
      {
        "matches": [ "<all_urls>" ],
        "js": [ "js/jquery-3.4.1.min.js", "js/content-script.js" ]
      }
    ]
  }